  98:lib/chibios/os/rt/src/chvt.c ****  */
  99:lib/chibios/os/rt/src/chvt.c **** static void vt_list_compress(virtual_timers_list_t *vtlp,
 100:lib/chibios/os/rt/src/chvt.c ****                              sysinterval_t deltanow) {
 101:lib/chibios/os/rt/src/chvt.c ****   virtual_timer_t *vtp = vtlp->next;
 102:lib/chibios/os/rt/src/chvt.c **** 
 103:lib/chibios/os/rt/src/chvt.c ****   /* The loop is bounded because the delta list header has the delta field
 104:lib/chibios/os/rt/src/chvt.c ****      set to (sysinterval_t)-1 which is larger than all deltas.*/
 105:lib/chibios/os/rt/src/chvt.c ****   while (vtp->delta < deltanow) {
 106:lib/chibios/os/rt/src/chvt.c ****     deltanow  -= vtp->delta;
 107:lib/chibios/os/rt/src/chvt.c ****     vtp->delta = (sysinterval_t)0;
 108:lib/chibios/os/rt/src/chvt.c ****     vtp        = vtp->next;
 109:lib/chibios/os/rt/src/chvt.c ****   }
 110:lib/chibios/os/rt/src/chvt.c **** 
 111:lib/chibios/os/rt/src/chvt.c ****   vtlp->lasttime = vtlp->lasttime + deltanow;
 112:lib/chibios/os/rt/src/chvt.c **** 
 113:lib/chibios/os/rt/src/chvt.c ****   /* Adjusting next timer in the list, if any.*/
 114:lib/chibios/os/rt/src/chvt.c ****   if (is_timer(vtlp, vtp)) {
 115:lib/chibios/os/rt/src/chvt.c ****     vtp->delta -= deltanow;
 116:lib/chibios/os/rt/src/chvt.c ****   }
 117:lib/chibios/os/rt/src/chvt.c **** }
 118:lib/chibios/os/rt/src/chvt.c **** #endif
